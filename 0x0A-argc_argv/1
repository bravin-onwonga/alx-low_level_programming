#include "main.h"

int strLen(char *c)
{
	int l;

	for (l = 0; c[l] != '\0'; l++)
		;
	return (l);
}

int intLen(int n)
{
	if (n / 10 == 0)
		return (1);
	return (1 + intLen(n / 10));
}

int main(int argc, char *argv[])
{
	int sum, num, i, str_len, len_num;
	
	if (argc == 1)
	{
		printf("%d\n", argc - 1);
		return (0);
	}

	for (i = 0; i < argc; i++)
	{
		if (*argv[i] >= 'a' && *argv[i] <= 'z')
		{
			printf("Error\n");
			return (1);
		}

		str_len = strLen(argv[i]); 
		num = atoi(argv[i]);
		if (num > 0)
		{
			len_num = intLen(num);
			if (len_num == str_len)
				sum += num;
			else
			{
				printf("Error\n");
				return (1);
			}
		}
	}
	printf("%d\n", sum);
	return (0);
}
